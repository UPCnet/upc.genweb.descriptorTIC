<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">

<head><title></title>
</head>
<body>
  <div metal:fill-slot="main">

      <metal:main_macro define-macro="main">

		<script type="text/javascript">

		function dobusqueda(){
			new Ajax.Request($('searchajax').action,{
				method:$('searchajax').method,
				parameters: $('searchajax').serialize(true),
				onSuccess: 	function(transport){ 
		  						$('ajaxresults').innerHTML = transport.responseText;
	  						}
			});
		}    

		</script>

      <div tal:replace="structure provider:plone.abovecontenttitle" />
      <div class="agendaContingut contacte" style="padding: 5px;">

      <h5 style="font-family: Arial,Helvetica,Verdana,sans-serif; font-size: 14px;">Cerca de Serveis per Colectius</h5>
      </div>

		<form action="searching_serveis"
		      class="enableAutoFocus"
		      method="get"
		      name="search"
		      id="searchajax"
		      tal:define="DateTime python:modules['DateTime'].DateTime">

		    <div class="container">

<!-- BUSQUEDA DE SERVEIS POR COLECTIVO-->

            <div>


                    <div class="formHelp" i18n:translate="help_search_title">
                    Return items matching this title.
                    </div>

                    <input type="text" onclick="dobusqueda();"
                           id="Title"
                           name="Title"
                           size="25"
                           tal:attributes="value request/SearchableText|nothing"
                           />

                    <label for="Title" i18n:translate="search_title"> &nbsp; Cerca per nom del Colectiu</label>                         
                    
            </div>

            <div class="field">
                <input
                   type="hidden"
                   name="portal_type:list"
                   value="ColectivoTIC"
                />
            </div>

		    </div>

            <div class="visualClear" id="clear-space-before-submit"><!-- --></div>
             
            <div id="ajaxresults"></div> 
		</form>

<div id="resultados"></div>

  </metal:main_macro>
 </div>
</body>
</html>


